{
  "name": "apache-jena-fuseki",
  "version": "1.0.0",
  "private": false,
  "description": "Apache Jena Fuseki UI",
  "author": "Apache Jena Team",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "test:unit": "vitest run --environment jsdom",
    "test:e2e": "cross-env ELECTRON_RUN_AS_NODE=\"1\" FUSEKI_PORT=\"${FUSEKI_PORT:=3030}\" PORT=\"${PORT:=8080}\" concurrently --names 'SERVER,CLIENT,TESTS' --prefix-colors 'yellow,blue,green' --success 'first' --kill-others 'yarn run serve:fuseki' 'yarn wait-on http://localhost:${FUSEKI_PORT}/$/ping && yarn run dev' 'yarn wait-on http-get://localhost:${PORT}/index.html && cypress run --headless --browser chromium $@'",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "coverage:unit": "yarn run test:unit --coverage",
    "coverage:e2e": "cross-env-shell CYPRESS_COVERAGE=true yarn run test:e2e",
    "serve:fuseki": "nodemon src/services/mock/json-server.js",
    "serve:offline": "cross-env ELECTRON_RUN_AS_NODE=\"1\" FUSEKI_PORT=\"${FUSEKI_PORT:=3030}\" PORT=\"${PORT:=8080}\" concurrently --names 'SERVER,CLIENT' --prefix-colors 'yellow,blue' --success 'first' --kill-others 'yarn run serve:fuseki' 'yarn wait-on http://localhost:${FUSEKI_PORT}/$/ping && yarn run dev'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "^2.11.6",
    "@triply/yasqe": "^4.2.28",
    "@triply/yasr": "^4.2.28",
    "@vue/compat": "^3.2.47",
    "axios": "^1.3.3",
    "bootstrap": "^5.2.3",
    "follow-redirects": "^1.15.2",
    "mitt": "^3.0.0",
    "nanoid": "^4.0.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-upload-component": "^3.1.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/vue": "^5.0.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.2.10",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.5.1",
    "cypress-vite": "^1.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "express": "^4.18.2",
    "jsdom": "^21.1.0",
    "json-server": "^0.17.1",
    "nodemon": "^2.0.20",
    "sass": "^1.58.0",
    "sinon": "^15.0.1",
    "vite": "^4.1.1",
    "vite-plugin-istanbul": "^4.0.0",
    "vitest": "^0.28.5",
    "wait-on": "^7.0.1"
  },
  "overrides": {
    "terser": "^5.14.2",
    "loader-utils": "^1.4.2",
    "cookiejar": "^2.1.4",
    "json5": "^1.0.2"
  }
}
