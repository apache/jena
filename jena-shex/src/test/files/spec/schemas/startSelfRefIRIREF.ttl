PREFIX sx: <http://www.w3.org/ns/shex#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

[] a sx:Schema ;
  sx:start <http://a.example/S1> ;
  sx:shapes (<http://a.example/S1>) .

