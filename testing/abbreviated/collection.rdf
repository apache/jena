<!-- MUST contain "rdf:parseType=?Collection? -->
<!-- MUST contain xmlns:owl=?http://www.w3.org/2002/07/owl#? -->

<rdf:RDF
  xmlns:rdf ="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>


<rdfs:Class rdf:ID="C">
    <daml:sameClassAs>
        <daml:Class>
          <daml:intersectionOf rdf:parseType="Collection">
            <daml:Class rdf:ID="A" />
            <daml:Class rdf:ID="B" />
          </daml:intersectionOf>
         </daml:Class>
    </daml:sameClassAs>
</rdfs:Class>


</rdf:RDF>
