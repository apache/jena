<!-- MUST contain "rdf:parseType=?Collection? -->
<!-- MUST contain xmlns:owl=?http://www.w3.org/2002/07/owl#? -->

<rdf:RDF
  xmlns:rdf ="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl ="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>


<rdfs:Class rdf:ID="C">
    <owl:sameClassAs>
        <owl:Class>
          <owl:intersectionOf rdf:parseType="Collection">
            <owl:Class rdf:ID="A" />
            <owl:Class rdf:ID="B" />
          </owl:intersectionOf>
         </owl:Class>
    </owl:sameClassAs>
</rdfs:Class>


</rdf:RDF>
