            [ qt:query  <opt-04.rq> ;
              qt:data   <data.ttl> ] ;
        mf:result  <opt-04.srx>
     ]
     [  mf:name    "Two optional, no fixed clause (1)" ;
        mf:action
            [ qt:query  <opt-05.rq> ;
              qt:data   <data.ttl> ] ;
        mf:result  <opt-05.srx>
