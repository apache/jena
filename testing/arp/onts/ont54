        <rdfs:subClassOf rdf:resource="#ChangeInUserRights"/>
        <rdfs:subClassOf rdf:resource="#SocialOccurrence"/>
        <daml:disjointWith rdf:resource="#GiftGiving"/>
    </daml:Class>
