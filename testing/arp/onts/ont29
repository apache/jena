    </daml:Class>

    <daml:Class rdf:ID="OvercastCloudLayer">
        <rdfs:label>Overcast</rdfs:label>
	<rdfs:comment>Cloud layers covering 8/8 of the sky. Coded with 'OVC'</rdfs:comment>
	<rdfs:subClassOf rdf:resource="#CloudLayer"/>
    </daml:Class>

    <daml:Class rdf:ID="CloudType">
        <rdfs:label>Cloud Type</rdfs:label>
	<rdfs:comment>At manual stations, cumulonimbus (CB) or towering cumulus (TCU) shall be appended to the associated layer.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="#WeatherQualifier"/>
    </daml:Class>

