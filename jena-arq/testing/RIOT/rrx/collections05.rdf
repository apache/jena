<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://local/" 
    xmlns:ex="http://example/" 
    xml:base="http://base/">

   <!-- Inline -->
  <rdf:Description rdf:about="http://example.org/basket4">
    <ex:hasFruit rdf:parseType="Collection">
      <rdf:Description rdf:about="http://example.org/banana"/>
      <rdf:Description rdf:about="http://example.org/apple">
        <ex:pips rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ex:pips>
      </rdf:Description>
    </ex:hasFruit>
  </rdf:Description>

</rdf:RDF>
