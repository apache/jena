PREFIX cdt:    <http://w3id.org/awslabs/neptune/SPARQL-CDTs/>
PREFIX xsd:    <http://www.w3.org/2001/XMLSchema#>

ASK {
	{
		SELECT * WHERE {
			VALUES (?id ?map) {
				(1  "{ <http://example.org/2> : 42 }"^^cdt:Map)
				(2  "{  <http://example.org/1> : 42 }"^^cdt:Map)
			}
		}
		ORDER BY ?map LIMIT 1
	}
	FILTER (?id = 2)
}
