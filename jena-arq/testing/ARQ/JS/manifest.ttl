@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt:     <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .

<>  rdf:type mf:Manifest ;
    rdfs:label    "JavaScript function" ;
    rdfs:comment  "JavaScript function" ;
    mf:entries
    ( 
      [  mf:name    "JS Call: file-defined function" ;
         mf:action
            [ qt:query  <js-query-1.rq> ;
              qt:data   <data.ttl> ] ;
         mf:result  <js-query-1.srj>
      ]
      [  mf:name    "JS Call: string-defined function" ;
         mf:action
            [ qt:query  <js-query-2.rq> ;
              qt:data   <data.ttl> ] ;
         mf:result  <js-query-2.srj>
      ]
      [  mf:name    "JS Call: nested functions" ;
         mf:action
            [ qt:query  <js-query-3.rq> ;
              qt:data   <data.ttl> ] ;
         mf:result  <js-query-3.srj>
      ]
      [  mf:name    "JS Call: FILTER functions" ;
         mf:action
            [ qt:query  <js-query-4.rq> ;
              qt:data   <data.ttl> ] ;
         mf:result  <js-query-4.srj>
      ]
      [  mf:name    "JS Call: FILTER functions" ;
         mf:action
            [ qt:query  <js-query-5.rq> ;
              qt:data   <data.ttl> ] ;
         mf:result  <js-query-5.srj>
      ]
    ).
