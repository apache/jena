<?xml version="1.0"?>

<tells
  xmlns="http://dl.kr.org/dig/lang"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://dl.kr.org/dig/lang http://potato.cs.man.ac.uk/dig/level0/dig.xsd">
  <defconcept name="http://example.org/foo#A" />
  <defconcept name="anon:" />
  <defrole name="http://example.org/foo#p" />
  
  <impliesc>
  	<catom name="http://example.org/foo#A" />
  	<catom name="anon:" />
  </impliesc>
  
  <equalc>
  	<catom name="anon:" />
  	<and>
		  <atmost num="1">
			  <ratom name="http://example.org/foo#p" />
			  <top />
		  </atmost>
		  <atleast num="1">
			  <ratom name="http://example.org/foo#p" />
			  <top />
		  </atleast>
  	</and>
  </equalc>
</tells>
